<ul>
    {{#each burgers}}
    {{#unless devoured}}

    <div class="card border border-success ready" id={{ this.id}}>
        <div class="card-body">
            <p><span> ID:</span> {{ this.id}}</p>
            <p><span> Food Item:</span> {{ this.burger_name}}</p>
            <p><span> IsDevoured:</span> {{ this.devoured}}</p>

            <button type="button" class="btn btn-info" data-myval={{ this.id}}>Devour it</button>

        </div>

    </div>

    <br>
    {{/unless}}
    {{/each}}
</ul>